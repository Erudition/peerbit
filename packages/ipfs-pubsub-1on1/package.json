{
  "name": "@dao-xyz/ipfs-pubsub-1on1",
  "version": "0.0.8",
  "description": "1-to-1 communication channel over IPFS Pubsub between two peers",
  "license": "MIT",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "ipfs",
    "log",
    "crdts",
    "crdt"
  ],
  "dependencies": {
    "@dao-xyz/borsh": "^3.2.5",
    "safe-buffer": "~5.2.1",
    "@dao-xyz/io-utils": "^0.0.1"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "assert": "^2.0.0",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "go-ipfs": "^0.10.0-rc1",
    "http-server": "^0.12.3",
    "ipfs": "^0.62.3",
    "ipfsd-ctl": "^10.0.3",
    "is-node": "^1.0.2",
    "jsdoc": "^3.6.6",
    "json-loader": "~0.5.7",
    "node-pre-gyp": "^0.13.0",
    "nyc": "^15.0.1",
    "orbit-db-test-utils": "^1.3.0",
    "rimraf": "~3.0.2",
    "standard": "~16.0.3",
    "webpack": "^5.37.1",
    "p-map-series": "^2.1.0"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | node ../../node_modules/.bin/json > lib/cjs/package.json && echo '{\"type\":\"module\"} ' | node ../../node_modules/.bin/json > lib/esm/package.json",
    "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.cjs --runInBand --forceExit",
    "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.cjs --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../jest.config.integration.cjs --runInBand --forceExit",
    "test:browser": "npm run build:tests && mocha-headless-chrome -t 360000 -f ./test/browser/index.html -a no-sandbox",
    "benchmark": "nyc  --require ts-node/register benchmark-runner -r -b --baselineLimit 1000",
    "benchmark:stress": "benchmark-runner -r --grep stress",
    "examples": "http-server examples/browser"
  },
  "localMaintainers": [
    "dao.xyz"
  ],
  "gitHead": "d10b89ef1a120a58e1a7e92aadcc2bb536a37e3e"
}