{
    "name": "@dao-xyz/peerbit-store",
    "version": "0.0.53",
    "description": "Modified base class for orbit db stores for Typescript with CJS and ESM builds",
    "type": "module",
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.mjs",
    "types": "lib/esm/index.d.ts",
    "exports": {
        "import": "./lib/esm/index.mjs",
        "require": "./lib/cjs/index.js"
    },
    "files": [
        "lib",
        "src",
        "!src/**/__tests__",
        "!lib/**/__tests__",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "shx rm -rf lib/*",
        "build": "yarn clean && tsc -p tsconfig.json && tsc-esm -p tsconfig.json",
        "postbuild": "echo '{\"type\":\"module\"} ' | node ../../node_modules/.bin/json > lib/esm/package.json",
        "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.ts --runInBand --forceExit",
        "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.ts --runInBand --forceExit",
        "test:integration": "node ../node_modules/.bin/jest test -c  ../../jest.config.integration.ts --runInBand --forceExit"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dao-xyz/peerbit"
    },
    "engines": {
        "node": ">=16.0"
    },
    "license": "MIT",
    "dependencies": {
        "@dao-xyz/ipfs-log": "^5.5.43",
        "@dao-xyz/peerbit-cache": "^0.4.17",
        "@dao-xyz/peerbit-io-utils": "^1.1.11",
        "p-each-series": "^3.0.0",
        "p-map": "^5.5.0",
        "p-queue": "^7.3.0"
    },
    "devDependencies": {
        "@dao-xyz/peerbit-keystore": "^0.0.25",
        "@dao-xyz/peerbit-test-utils": "^0.0.12",
        "go-ipfs": "^0.16.0",
        "ipfs": "^0.65.0",
        "ipfsd-ctl": "^12.2.2",
        "level": "^8.0.0",
        "libsodium-wrappers": "^0.7.10"
    },
    "localMaintainers": [
        "dao.xyz"
    ],
    "gitHead": "63c806ffbe224816cf72fc5a7859f3437558cada"
}
