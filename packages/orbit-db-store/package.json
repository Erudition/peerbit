{
  "name": "@dao-xyz/orbit-db-store",
  "version": "0.0.7",
  "description": "Modified base class for orbit db stores for Typescript with CJS and ESM builds",
  "type": "module",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | npx json > lib/cjs/package.json && echo '{\"type\":\"module\"} ' | npx json > lib/esm/package.json",
    "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.cjs --runInBand --forceExit",
    "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.cjs --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../jest.config.integration.cjs --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orbitdb/orbit-db-store"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Haad",
  "license": "MIT",
  "dependencies": {
    "@dao-xyz/ipfs-log": "^5.5.5",
    "it-to-stream": "^1.0.0",
    "logplease": "^1.2.14",
    "p-each-series": "^2.1.0",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "readable-stream": "~3.6.0"
  },
  "devDependencies": {
    "go-ipfs": "^0.9.1",
    "ipfs": "^0.62.3",
    "ipfsd-ctl": "^10.0.3",
    "memdown": "^6.0.0",
    "orbit-db-cache": "~0.3.0",
    "orbit-db-identity-provider": "~0.3.1",
    "orbit-db-io": "^1.0.1",
    "orbit-db-keystore": "~0.3.5",
    "orbit-db-storage-adapter": "^0.6.0",
    "orbit-db-test-utils": "^1.3.0"
  },
  "peerDependencies": {
    "orbit-db-io": "*"
  },
  "localMaintainers": [
    "dao.xyz"
  ],
  "gitHead": "62040f90296fad19bc5275a096e7523b28fae116"
}
