{
  "name": "@dao-xyz/orbit-db-identity-provider",
  "version": "0.5.10",
  "description": "Identity provider for OrbitDB",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | node ../../node_modules/.bin/json > lib/cjs/package.json && echo '{\"type\":\"module\"} ' | node ../../node_modules/.bin/json > lib/esm/package.json",
    "test": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.cjs --runInBand --forceExit",
    "test:unit": "node ../../node_modules/.bin/jest test  -c  ../../jest.config.unit.cjs --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../jest.config.integration.cjs --runInBand --forceExit",
    "test:browser": "npm run build:tests && mocha-headless-chrome -f ./test/browser/index.html -a no-sandbox"
  },
  "keywords": [
    "orbitdb",
    "orbit-db",
    "identity"
  ],
  "author": "shamb0t",
  "contributors": [
    "shamb0t",
    "haadcode",
    "RichardLitt",
    "latenssi",
    "adam-palazzo"
  ],
  "license": "MIT",
  "devDependencies": {
    "fs-extra": "^10.0.0",
    "is-node": "^1.0.2",
    "json-loader": "^0.5.7",
    "key-did-provider-ed25519": "^1.0.0",
    "key-did-resolver": "^1.2.1",
    "mocha-headless-chrome": "^3.1.0",
    "process": "^0.11.10",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "@solana/web3.js": "^1.41.3",
    "bs58": "^5.0.0",
    "@dao-xyz/borsh": "^3.2.2",
    "@dao-xyz/identity": "^0.0.12",
    "@ethersproject/wallet": "^5.7.0",
    "assert": "^2.0.0",
    "dids": "^2.1.0",
    "@dao-xyz/orbit-db-keystore": "^0.0.1",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "tweetnacl": "^1.0.3"
  },
  "localMaintainers": [
    "haad <haad@haja.io>",
    "shamb0t <shams@haja.io>",
    "hajamark <mark@haja.io>"
  ],
  "gitHead": "d10b89ef1a120a58e1a7e92aadcc2bb536a37e3e"
}