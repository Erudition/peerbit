{
  "name": "@dao-xyz/peerbit-query",
  "version": "0.0.40",
  "description": "Cross peer com. primitives",
  "type": "module",
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.mjs",
  "types": "lib/esm/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.mjs",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "src",
    "!src/**/__tests__",
    "!lib/**/__tests__",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf lib/*",
    "build": "yarn clean && tsc -p tsconfig.json; tsc-esm -p tsconfig.json",
    "postbuild": "echo '{\"type\":\"module\"} ' | node ../../../../node_modules/.bin/json > lib/esm/package.json",
    "test": "node ../../../../node_modules/.bin/jest test  -c  ../../../../jest.config.ts --runInBand --forceExit",
    "test:unit": "node ../../../../node_modules/.bin/jest test  -c  ../../../../jest.config.unit.ts --runInBand --forceExit",
    "test:integration": "node ../node_modules/.bin/jest test -c  ../../../../jest.config.integration.ts --runInBand --forceExit"
  },
  "author": "dao.xyz",
  "license": "MIT",
  "dependencies": {
    "@dao-xyz/borsh": "^3.4.2",
    "@dao-xyz/peerbit-bpayload": "^0.0.19",
    "@dao-xyz/peerbit-crypto": "^0.0.14",
    "@dao-xyz/peerbit-program": "^0.0.37",
    "@dao-xyz/peerbit-store": "^0.0.52",
    "@dao-xyz/peerbit-time": "^0.0.17",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "ipfs": "^0.65.0"
  },
  "gitHead": "63c806ffbe224816cf72fc5a7859f3437558cada"
}
