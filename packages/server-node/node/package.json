{
    "name": "@dao-xyz/peerbit-node",
    "version": "0.0.108",
    "author": "dao.xyz",
    "repository": "https://github.com/@dao-xyz/peerbit-node",
    "license": "Apache-2.0",
    "type": "module",
    "sideEffects": false,
    "bin": {
        "peerbit": "./lib/esm/bin.js"
    },
    "module": "lib/esm/index.js",
    "types": "lib/esm/index.d.ts",
    "exports": {
        "import": "./lib/esm/index.js"
    },
    "files": [
        "lib",
        "src",
        "src/nginx-template.conf",
        "!src/**/__tests__",
        "!lib/**/__tests__",
        "patches",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16.15.1"
    },
    "scripts": {
        "clean": "shx rm -rf lib/*",
        "build": "yarn clean && yarn build-lib && yarn build-ui",
        "build-lib": "tsc -p tsconfig.json",
        "build-ui": "cd ../frontend && yarn build-prod && cd ../node",
        "postbuild": "cp src/nginx-template.conf lib/esm/ && echo '{\"type\":\"module\"} ' | node ../../../node_modules/.bin/json > lib/esm/package.json && cp -r ../frontend/build lib/ui",
        "test": "node ../../../node_modules/.bin/jest test  -c  ../../../jest.config.ts --runInBand --forceExit",
        "test:unit": "node ../../../node_modules/.bin/jest test  -c  ../../../jest.config.unit.ts --runInBand --forceExit",
        "test:integration": "node ../node_modules/.bin/jest test -c  ../../../jest.config.integration.ts --runInBand --forceExit",
        "postinstall": "patch-package || exit 0"
    },
    "devDependencies": {
        "@dao-xyz/peerbit-node-test-lib": "^0.0.1",
        "@dao-xyz/peerbit-string": "^0.0.146",
        "@dao-xyz/peerbit-test-utils": "^0.0.25",
        "@types/yargs": "^17.0.13",
        "aws-sdk": "^2.1259.0",
        "axios": "^1.1.3",
        "dotenv": "^16.0.3",
        "yargs": "^17.6.0"
    },
    "dependencies": {
        "@aws-sdk/client-route-53": "^3.216.0",
        "@chainsafe/libp2p-gossipsub": "^5.3.0",
        "@chainsafe/libp2p-noise": "^10.2.0",
        "@dao-xyz/peerbit": "^0.0.102",
        "@libp2p/floodsub": "^5.0.0",
        "@libp2p/mplex": "^7.1.1",
        "@libp2p/tcp": "^6.0.8",
        "@libp2p/websockets": "^5.0.2",
        "is-node": "^1.0.2",
        "libp2p": "^0.41.0"
    },
    "peerDependencies": {
        "axios": "^1.1.3",
        "yargs": "^17.6.0"
    },
    "gitHead": "bfbec720aee276ab1bc7cf4ee7a6c0e90ecb4f51"
}
